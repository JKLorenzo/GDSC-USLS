{
  "name": "gdsc-usls",
  "version": "1.0.0",
  "description": "A Discord Bot for Google Developer Student Clubs of University of St. La Salle.",
  "main": "dist/client.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "is-ci || rimraf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "postinstall": "npm run clean && npm run build",
    "prepare": "is-ci || husky install",
    "prettier": "prettier --check **/*.{json,yml,yaml}",
    "prettier:fix": "prettier --write **/*.{json,yml,yaml}"
  },
  "engines": {
    "node": "16.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JKLorenzo/GDSC-USLS.git"
  },
  "keywords": [
    "nodejs",
    "discord-bot",
    "gdsc-usls"
  ],
  "author": "Juruel Keanu Lorenzo <keanulorenzo32@gmail.com>",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/JKLorenzo/GDSC-USLS/issues"
  },
  "homepage": "https://github.com/JKLorenzo/GDSC-USLS#readme",
  "dependencies": {
    "common-tags": "^1.8.0",
    "discord.js": "^13.1.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/common-tags": "^1.8.1",
    "@types/lodash": "^4.14.175",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "is-ci": "^3.0.0",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  }
}
